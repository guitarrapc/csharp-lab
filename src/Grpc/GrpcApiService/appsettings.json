{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Grpc.AspNetCore.Server": "Error" // Suppress log message "Grpc.AspNetCore.Server.ServerCallHandler[14]: The client reset the request stream"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Protocols": "Http1AndHttp2",
        "Url": "http://+:5095"
      }
    },
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
